{
  "comments": {
    "blockComment": ["<%--", "--%>"]
  },
  "brackets": [
    ["<!--", "-->"],
    ["<", ">"],
    ["{", "}"],
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "'", "close": "'", "notIn": ["string"] },
    { "open": "<%", "close": "%>" },
    { "open": "<%=", "close": "%>" },
    { "open": "<%!", "close": "%>" },
    { "open": "<%@", "close": "%>" },
    { "open": "<!--", "close": "-->" },
    { "open": "<![CDATA[", "close": "]]>" }
  ],
  "surroundingPairs": [
    ["<", ">"],
    ["{", "}"],
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*<%(?![=@!])",
      "end": "\\s*%>\\s*$"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "(<(?!\\?|[^>]*\\/>)[^>]*>|\\{[^}\"']*$)|<%(?![=!@])|<%[^%]*$",
    "decreaseIndentPattern": "(</[^>]+>|^\\s*-->|^\\s*<\\?\\s*end|^\\s*%>)"
  }
}
